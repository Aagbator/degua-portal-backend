{
  "name": "degua-portal-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "s": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "nodemon --exec node ./dist/index.js",
    "dev": "nodemon --exec \"ts-node -r tsconfig-paths/register\" src/index.ts",
    "clean": "rm -rf ./dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "scripts": {
    "test": "jest --coverage",
    "dev": "NODE_ENV=development nodemon src/index.ts",
    "prod": "NODE_ENV=production node src/index.js",
    "seed": "npx sequelize db:seed:all",
    "build": "npx tsc",
    "clean": "rm -rf ./dist",
    "apply-seeders": "node -r dotenv-flow/config ./node_modules/.bin/sequelize db:seed:all",
    "revert-seeders": "node -r dotenv-flow/config ./node_modules/.bin/sequelize db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aagbator/degua-portal-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Aagbator/degua-portal-backend/issues"
  },
  "homepage": "https://github.com/Aagbator/degua-portal-backend#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.1.5",
    "morgan": "^1.10.0",
    "mysql2": "^3.2.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.31.0",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.1",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0"
  }
}
